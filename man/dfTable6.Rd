\name{Table6}
\alias{Table6}
\title{Table6 dataset}
\format{a data.frame with 8 observations of the following variables, no NAs
\describe{
\item{y}{number positive}
\item{n}{number positive + number negative}
\item{tx}{treatment 'a' or 'b'}
\item{clus}{cluster ID}
}}
\description{
